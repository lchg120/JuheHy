.matchViewBox{position: relative;width:100%; height: 100%; overflow: hidden; color: #fff; background-color:#673ab7;}
.waitView{position: absolute; top: 0; left: 0; z-index: 1; width:100%; height:100%; font-size: 28rpx;}
.loading{width:17px; display: inline-block}
.loading:after {vertical-align: bottom; animation: ellipsis 1s infinite; content: "\2026"; overflow: hidden; display: inline-block;}
@keyframes ellipsis {from {width:2px;}to{width: 17px;}}

