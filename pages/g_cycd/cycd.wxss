.vsViewQuestion{position: absolute; width:620rpx; top:80rpx; left: calc((100% - 600rpx)/2)}
.questionItem{width:155rpx; height: 126rpx; position: relative}
.questionItem image{width:126rpx; height: 126rpx;}
.questText{position: absolute; left: 0; top:0; width:100%; padding-top:20rpx; text-align: center; font-size: 60rpx;color:#fff;}
.wenhao{position: absolute; left: 46rpx; top:26rpx;}

.vsViewAnswer{position: absolute; width:600rpx; top: 390rpx; left: calc((100% - 600rpx)/2)}
