.fightViewLeft .userArea{position: absolute; width:154rpx; left: 4rpx; top: 0rpx;}
.fightViewLeft .userScore{position:absolute; left: 162rpx; top: 65rpx; font-size: 40rpx; color:#f5750e; font-weight: 800; width: 110rpx; text-align: center}
.fightViewLeft .addScore{position:absolute; left: 162rpx; top: 58rpx; font-size: 40rpx; font-weight: 800; width: 110rpx; text-align: center; opacity: 0}

.fightViewUser .userArea .avatar{height: 150rpx;width:150rpx; border: 8rpx solid #cfcfcf; -webkit-border-radius: 50%; border-radius: 50%;}
.fightViewUser .userArea .avatar image{width:100%; height: 100%; border-radius: 50%;}
.fightViewUser .userArea .userName{width:100%; height:90rpx; line-height: 30rpx; font-size: 24rpx; margin-top:18rpx; text-align: center; color: #fff;}
.fightViewRight{position: absolute; width:288rpx; height: 100%; left: 404rpx; top: 0; animation-duration:18rpx; }
.fightViewRight .userScore{position:absolute; left: 12rpx; top: 65rpx; font-size: 40rpx; color:#f5750e; font-weight: 800; width: 110rpx; text-align: center;}
.fightViewRight .addScore{position:absolute; left: 12rpx; top: 58rpx; font-size: 40rpx; font-weight: 800; width: 110rpx; text-align: center; opacity: 0}

.fightViewRight .userArea{position: absolute; width:154rpx; left: 132rpx; top: 0rpx;}

