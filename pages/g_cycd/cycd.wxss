.shateBtn{top:856rpx; background: #ff8022;}
.shareTip{width:100%; position:absolute; top:1026rpx; font-size: 28rpx;}
.lvUp {
  position: absolute; top: 0; left: 0; margin: auto; z-index: 3; width: 100%; height: 100%; background-color: rgba(0, 0, 0, .5);
}
.lvUp.hide {
  display: none;
}
.lvUpBg {
  margin: 140rpx auto 0;
  width: 78vw;
  height: 60vh;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
.lvUpText {
  padding-top: 260rpx; 
  font-size: 34rpx;
  color: #383838;
  text-align: center;
}
.lvUpText .text1{
  font-size: 68rpx;
  font-weight: bold;
  color: #ff3f73;
}
.lvUpText .text2 {
  margin: 30rpx auto;
  position: relative;
  width: 60vw;
  border: 1px solid #c7c7c7;
  color: #c9c9c9;
}
.lvUpText .text2 text {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  margin: auto;
  width: 200rpx;
  height: 50rpx;
  color: 52rpx;
  background: #fff;
}
.lvUpText .state {
  color: #022660;
}
.lvUpText .btn {
  margin: 20rpx auto 0;
  width: 394rpx;
  height: 100rpx;
  background: #ff8023;
  color: #fff;
  -webkit-border-radius: 20rpx;
  border-radius: 20rpx;
  border: none;
  box-shadow: 0 10rpx 8rpx rgba(0,0,0,0.5);
}

@keyframes cirleLine {
	from{transform: rotate(0deg);}to{transform:rotate(30deg);}
	from, 50%, to {opacity: 1;scale:(1.2,1.2)}25%, 75% {opacity: 0;scale:1}
}
@-webkit-keyframes cirleLine {
	from{transform: rotate(0deg);}to{transform:rotate(30deg);}
	from, 50%, to {opacity: 1;scale:(1.2,1.2)}25%, 75% {opacity: 0;scale:1}
}

.btn-back {
  margin-top: 50rpx;
  width: 300rpx;
  height: 60rpx;
  line-height: 60rpx;
  webkit-border-radius: 10rpx;
  border: none;
  border-radius: 10rpx;
  background: #fff;
  text-align: center;
  text-size: 16rpx;
  color: #673ab7;

}

