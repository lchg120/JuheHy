.showGlod{width:200rpx; position: absolute; left: calc((100% - 387rpx)/2); top: 556rpx;}
.glodNumShow{padding: 6rpx 0 0 20rpx; font-size: 40rpx}
.showExp{width:200rpx; position: absolute; top: 556rpx; left: calc((100% - 387rpx));}
